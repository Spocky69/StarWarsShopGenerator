<Window x:Class="StarWarsShopGenerator.View.NewFileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StarWarsShopGenerator.View"
        mc:Ignorable="d"
        Title="Nouveau Fichier" Height="81" Width="307" ResizeMode="NoResize" ShowInTaskbar="False" WindowStyle="None" Foreground="{x:Null}">
	<Border BorderBrush="White" BorderThickness="1,1,0,0">
		<Grid>
			<StackPanel VerticalAlignment="Center">
				<Grid Height="37">
					<Label Content="Nom du Fichier :" HorizontalAlignment="Left" Margin="1,1,0,0" VerticalAlignment="Top"/>
					<TextBox x:Name="TxtInput" Margin="98,5,10,10" ForceCursor="True" MaxLines="1"/>
				</Grid>
				<Grid Height="30" Width="306">
					<Button x:Name="Cancel" Content="Annuler" Margin="179,0,0,0" HorizontalAlignment="Left" Width="51" Height="20" VerticalAlignment="Center" Click="Button_Cancel" IsCancel="True" />
					<Button x:Name="OK" Content="OK" HorizontalAlignment="Left" Margin="85,0,0,0" Click="Button_OK" Width="51" Height="20" VerticalAlignment="Center" IsDefault="True"/>
				</Grid>
			</StackPanel>
		</Grid>
	</Border>
</Window>
